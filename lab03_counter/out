 .timescale 0 0;
S_000001e21c612ae0 .scope module, "top" "top" 3 4;
 .timescale 0 0;
v000001e21c338bf0_0 .var/2u "clk", 0 0;
v000001e21c339f80_0 .var "cnt_2bits", 1 0;
v000001e21c33b200_0 .var "cnt_3bits", 2 0;
v000001e21c33b2a0_0 .var/2s "cnt_int", 31 0;
E_000001e21c33da10 .event posedge, v000001e21c338bf0_0;
    .scope S_000001e21c612ae0;
T_0 ;
    %pushi/vec4 0, 0, 2;
    %store/vec4 v000001e21c339f80_0, 0, 2;
    %pushi/vec4 0, 0, 3;
    %store/vec4 v000001e21c33b200_0, 0, 3;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v000001e21c33b2a0_0, 0, 32;
    %end;
    .thread T_0, $init;
    .scope S_000001e21c612ae0;
T_1 ;
    %delay 5, 0;
    %load/vec4 v000001e21c338bf0_0;
    %inv;
    %store/vec4 v000001e21c338bf0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_000001e21c612ae0;
T_2 ;
    %wait E_000001e21c33da10;
    %load/vec4 v000001e21c339f80_0;
    %addi 1, 0, 2;
    %assign/vec4 v000001e21c339f80_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_000001e21c612ae0;
T_3 ;
    %wait E_000001e21c33da10;
    %load/vec4 v000001e21c33b200_0;
    %addi 1, 0, 3;
    %assign/vec4 v000001e21c33b200_0, 0;
    %jmp T_3;
    .thread T_3;
    .scope S_000001e21c612ae0;
T_4 ;
    %wait E_000001e21c33da10;
    %load/vec4 v000001e21c33b2a0_0;
    %addi 1, 0, 32;
    %cast2;
    %assign/vec4 v000001e21c33b2a0_0, 0;
    %jmp T_4;
    .thread T_4;
    .scope S_000001e21c612ae0;
T_5 ;
    %vpi_call/w 3 25 "$dumpfile", "top.vcd" {0 0 0};
    %vpi_call/w 3 26 "$dumpvars" {0 0 0};
    %delay 1000, 0;
    %vpi_call/w 3 29 "$display", "at time = ", $time, " end of the sim" {0 0 0};
    %vpi_call/w 3 30 "$finish" {0 0 0};
    %end;
    .thread T_5;
