 .timescale 0 0;
S_00000221f5252d90 .scope module, "top" "top" 3 8;
 .timescale 0 0;
v00000221f4efa340_0 .var "clk", 0 0;
v00000221f4ef94c0_0 .var/2u "my_bit", 0 0;
v00000221f4ef9220_0 .var/2s "my_int", 31 0;
v00000221f52530b0_0 .var/i "my_integer", 31 0;
v00000221f5253150_0 .var/str "my_str";
v00000221f5284410_0 .var "my_struct1", 1 0;
v00000221f52844b0_0 .var "my_struct2", 1 0;
S_00000221f5252f20 .scope function.vec4.s1, "my_func" "my_func" 3 24, 3 24 0, S_00000221f5252d90;
 .timescale 0 0;
; Variable my_func is vec4 return value of scope S_00000221f5252f20
TD_top.my_func ;
    %end;
S_00000221f5284280 .scope task, "my_task" "my_task" 3 28, 3 28 0, S_00000221f5252d90;
 .timescale 0 0;
TD_top.my_task ;
    %vpi_call/w 3 30 "$display", "Start task, and will force clk to 0 after #1" {0 0 0};
    %delay 1, 0;
    %pushi/vec4 0, 0, 1;
    %force/vec4 v00000221f4efa340_0;
    %vpi_call/w 3 33 "$display", "Finish task" {0 0 0};
    %end;
    .scope S_00000221f5252d90;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v00000221f4efa340_0, 0, 1;
    %end;
    .thread T_2, $init;
    .scope S_00000221f5252d90;
T_3 ;
    %delay 5, 0;
    %load/vec4 v00000221f4efa340_0;
    %inv;
    %store/vec4 v00000221f4efa340_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_00000221f5252d90;
T_4 ;
    %vpi_call/w 3 37 "$dumpfile", "top.vcd" {0 0 0};
    %vpi_call/w 3 38 "$dumpvars" {0 0 0};
    %vpi_call/w 3 41 "$display", "at time = %0d:", $time, "start sim" {0 0 0};
    %delay 40, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v00000221f4ef94c0_0, 0, 1;
    %pushi/vec4 30, 0, 32;
    %store/vec4 v00000221f4ef9220_0, 0, 32;
    %pushi/vec4 40, 0, 32;
    %store/vec4 v00000221f52530b0_0, 0, 32;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v00000221f5284410_0, 4, 1;
    %pushi/vec4 0, 0, 1;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v00000221f5284410_0, 4, 1;
    %pushi/vec4 1, 0, 1;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v00000221f52844b0_0, 4, 1;
    %pushi/str "this is a sentence.";
    %store/str v00000221f5253150_0;
    %fork TD_top.my_task, S_00000221f5284280;
    %join;
    %delay 50, 0;
    %vpi_call/w 3 55 "$display", "at time = %0d:", $time, "end sim" {0 0 0};
    %vpi_call/w 3 56 "$finish" {0 0 0};
    %end;
    .thread T_4;
